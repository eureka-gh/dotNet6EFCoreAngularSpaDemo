{
  "Logging": { // If No provider, LogLevel applies to all the enabled providers.
    "LogLevel": {
      // Trace = 0, Debug = 1, Information = 2, Warning = 3, Error = 4, Critical = 5, and None = 6.
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": { // logger provider - azure app insights
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
